{
  "name": "vite-react-boilerplate",
  "author": "Ricardo Valdovinos <ricardovaldovinos1996@gmail.com>",
  "description": "A production ready, batteries included starter template for Vite + React projects",
  "keywords": [
    "vite",
    "react",
    "boilerplate",
    "starter",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RicardoValdovinos/vite-react-boilerplate"
  },
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "setup": "git init && npx husky init && npx playwright install && rm .husky/pre-commit",
    "format": "prettier src/**/*.{ts,tsx} --write",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "dev": "vite",
    "storybook": "storybook dev -p 6006",
    "storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
    "storybook:build": "storybook build",
    "test": "vitest run src/ && playwright test",
    "test:unit": "vitest src/",
    "test:unit:coverage": "vitest --coverage src/",
    "test:e2e": "playwright test",
    "test:e2e:report": "playwright show-report",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@nivo/bar": "^0.87.0",
    "@nivo/core": "^0.87.0",
    "@nivo/line": "^0.87.0",
    "@nivo/pie": "^0.87.0",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-router": "^1.47.1",
    "@tanstack/react-table": "^8.20.1",
    "dayjs": "^1.11.12",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^15.0.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^8.4.1",
    "@headlessui/react": "^2.1.2",
    "@heroicons/react": "^2.1.5",
    "@hookform/devtools": "^4.3.1",
    "@playwright/test": "^1.46.0",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-themes": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/react-vite": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@tanstack/react-query-devtools": "^5.51.23",
    "@tanstack/react-table-devtools": "^8.20.1",
    "@tanstack/router-devtools": "^1.47.1",
    "@tanstack/router-plugin": "^1.47.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "@vitest/coverage-v8": "2.0.5",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jsdom": "^24.1.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prop-types": "^15.8.1",
    "storybook": "^8.2.8",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "5.4.0",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "2.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
